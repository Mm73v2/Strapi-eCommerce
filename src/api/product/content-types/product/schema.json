{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "banner": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "price": {
      "type": "decimal"
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "Electronics",
        "Fashion",
        "Home",
        "Toys"
      ]
    },
    "discount": {
      "type": "decimal"
    },
    "isNew": {
      "type": "boolean"
    },
    "rate": {
      "type": "decimal"
    },
    "reviews": {
      "type": "integer"
    }
  }
}
